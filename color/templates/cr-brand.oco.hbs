/name: Code Rabbit Brand Palette
/version: {{version}}
/source: https://github.com/coderabbit-team/design/color

{{#each general as |color|}}
{{capitalize color.name}}: {{color.hex}}
{{/each}}
{{#each colors as |color|}}
{{capitalize color.name}}s:
    {{#each color.hex as |hex index|}}
  {{index}}: {{hex}}
    {{/each}}
{{/each}}

Fully Saturated:
{{#each colors as |color|}}
  {{color.name}}: ={{capitalize color.name}}s.9
{{/each}}